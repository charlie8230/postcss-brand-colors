{
  "name": "postcss-brand-colors",
  "version": "0.4.0",
  "description": "PostCSS plugin to insert branding colors of all the major companies",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "branding",
    "brand colors",
    "brand-colors",
    "colors"
  ],
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/postcss/postcss-brand-colors.git"
  },
  "dependencies": {
    "brand-colors": "^1.1.2",
    "postcss": "^5.0.14"
  },
  "devDependencies": {
    "eslint": "^1.10.3",
    "eslint-config-postcss": "^1.0.0",
    "jest": "^20.0.0"
  },
  "scripts": {
    "test": "jest --coverage && eslint *.js"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 100
      }
    }
  },
  "eslintConfig": {
    "extends": "eslint-config-postcss/es5",
    "env": {
      "jest": true
    }
  }
}
